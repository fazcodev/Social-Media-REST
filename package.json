{
  "name": "social-media-rest",
  "version": "1.0.0",
  "description": "A social media application rest api",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Mohd-Adil-JMI/social-media-REST.git"
  },
  "keywords": [
    "mongodb",
    "express",
    "node",
    "js",
    "socialmedia",
    "mongoose"
  ],
  "author": "Mohd-Adil-JMI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mohd-Adil-JMI/social-media-REST/issues"
  },
  "homepage": "https://github.com/Mohd-Adil-JMI/social-media-REST#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.345.0",
    "@aws-sdk/s3-request-presigner": "^3.345.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemon": "^3.0.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
